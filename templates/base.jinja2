    {# Include custom css #}
    <link href="/{{ url_prefix }}/styles/custom.css" rel="stylesheet" crossorigin="anonymous">

    {# Title is set by defining the title block in child templates #}
    <title>{% block title %}{{ title }}{% endblock title %}</title>
</head>

<body>

